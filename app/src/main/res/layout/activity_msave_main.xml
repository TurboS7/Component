<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MSaveMainActivity">

  <Button
      android:id="@+id/btn_SharedPreferences"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toSharedPreferences"
      android:layout_marginTop="10dp"
      android:text="SharedPreferences" />
  <Button
      android:id="@+id/btn_SQLite"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toSQLite"
      android:layout_marginTop="10dp"
      android:text="btn_SQLite" />
</LinearLayout>